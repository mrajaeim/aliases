alias nab="nano ~/.bashrc"
alias cl="clear"

# Docker
alias d="docker"
alias dci="docker commit"
alias dc="docker-compose"
alias dcud="docker-compose up -d"
alias dps="docker ps"
alias dpsa="docker ps -a"
alias dex="docker exec -it"
alias dexsu="docker exec -it -u 0"


# git
alias gad="git add"
alias gbr="git branch"
alias gci="git commit -m"
alias gco="git checkout"
alias gfe="git fetch origin"
alias gpl="git pull"
alias gpp="git stash pop"
alias gpu="git push"
alias gsh="git stash -m"
alias gshl="git stash list"
alias gst="git status"
alias gsw="git switch"
alias greh="git reset --hard"

# dev
alias npi="npm install"
alias npui="npm uninstall"
alias npdv="npm run dev"
alias npsb="npm run storybook"

# Network
alias myip="curl ifconfig.me"
alias sysip="ipconfig"
